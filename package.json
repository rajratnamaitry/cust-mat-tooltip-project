{
  "name": "cust-mat-tooltip-project",
  "version": "1.0.0",
  "description": "An Angular library that extends Angular Material's tooltip with HTML content, custom positions, manual control, and accessibility support.",
  "author": "rajratna maitry",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajratnamaitry/MatCustomTooltip.git"
  },
  "keywords": [
    "angular",
    "material",
    "tooltip",
    "custom",
    "html",
    "directive"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build MatCustomTooltip --watch --configuration development",
    "build:lib": "ng build MatCustomTooltip",
    "test:lib": "ng test MatCustomTooltip",
    "publish:lib": "npm publish --access public",
    "unpublish": "npm unpublish mat-custom-tooltip@2.0.0 --force",
    "deploy": "ng build --base-href /cust-mat-tooltip-project/ && npx gh-pages -d dist/cust-mat-tooltip-project -b gh-pages"
  },
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "mat-custom-tooltip": "^1.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular/cli": "~15.2.11",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "gh-pages": "^6.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.2.2",
    "typescript": "~4.9.4"
  }
}
